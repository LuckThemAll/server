include/master-slave.inc
[connection master]
create table t1 (a int, b int) engine=InnoDB;
xa start 't';
insert into t1 values(1, 2);
xa end 't';
xa prepare 't';
xa commit 't';
select * from t1;
a	b
1	2
connection slave;
select * from t1;
a	b
1	2
connection master;
xa start 't';
insert into t1 values(3, 4);
xa end 't';
xa prepare 't';
xa rollback 't';
select * from t1;
a	b
1	2
connection slave;
select * from t1;
a	b
1	2
connection master;
drop table t1;
include/rpl_end.inc
